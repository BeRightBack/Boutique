<!DOCTYPE html>
<html lang="en">

<head>
    <!-- Required meta tags -->
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <title>Admin </title>
    <!-- plugins:css -->
    <link rel="stylesheet" href="~/template2/vendors/feather/feather.css">
    <link rel="stylesheet" href="~/template2/vendors/mdi/css/materialdesignicons.min.css">
    <link rel="stylesheet" href="~/lib/bootstrap-icons/dist/font/bootstrap-icons.css">
    <link rel="stylesheet" href="~/lib/font-awesome/dist/css/all.css" />
    <link rel="stylesheet" href="~/template2/vendors/ti-icons/css/themify-icons.css">
    <link rel="stylesheet" href="~/template2/vendors/typicons/typicons.css">
    <link rel="stylesheet" href="~/template2/vendors/simple-line-icons/css/simple-line-icons.css">
    <link rel="stylesheet" href="~/template2/vendors/css/vendor.bundle.base.css">
    <link rel="stylesheet" href="~/@(nameof(Boutique)).styles.css" asp-append-version="true" />
    <!-- endinject -->
    <!-- Plugin css for this page -->
    
    
    <!-- End plugin css for this page -->
    <!-- inject:css -->
    <link rel="stylesheet" href="~/template2/css/vertical-layout-light/style.css">
    <!-- endinject -->
    <link rel="shortcut icon" href="~/favicon.ico" />
    <!-- TinyMCE -->
    <script src="~/lib/tiny/tinymce.js" referrerpolicy="origin"></script>
    <script>
        tinymce.init({
            selector: 'textarea#tiny',
            plugins: [
                'file','save','a11ychecker', 'advlist', 'advcode', 'advtable', 'autolink', 'checklist', 'export',
                'lists', 'link', 'image', 'charmap', 'preview', 'anchor', 'searchreplace', 'visualblocks',
                'powerpaste', 'fullscreen', 'formatpainter', 'insertdatetime', 'media', 'table', 'help', 'wordcount'
            ],
            toolbar: 'save undo redo | a11ycheck casechange blocks | bold italic backcolor | alignleft aligncenter alignright alignjustify |' +
                'bullist numlist checklist outdent indent | removeformat | code table help'
        })
    </script>
</head>

<body>
    <!-- partial:partials/_navbar.html -->
    <partial name="_navbar" />
    <!-- partial -->
    <div class="container-fluid page-body-wrapper">
        <!-- partial:partials/_settings-panel.html -->
        <partial name="_settings-panel" />
        <!-- partial -->
        <!-- partial:partials/_sidebar.html -->
        <partial name="_sidebar" />
        <!-- partial -->

        <div class="main-panel">
            <div class="content-wrapper">
                <div class="row">
                    <div class="col-sm-12">
                        <div class="home-tab"></div>
                        <div class="container-fluid">
                            @RenderBody()
                        </div>
                    </div>
                </div>
            </div>
        </div></div>

        <!-- content-wrapper ends -->
        <!-- partial:partials/_footer -->
        <partial name="_Footer" />

        <!-- partial -->
    </div>
    <!-- main-panel ends -->
    </div>
    <!-- page-body-wrapper ends -->
    </div>
    <!-- container-scroller -->

    <!-- plugins:js -->
    <script src="~/template2/vendors/js/vendor.bundle.base.js"></script>
    <!-- endinject -->
    <!-- Plugin js for this page -->
    <script src="~/template2/vendors/chart.js/Chart.min.js"></script>
    <script src="~/template2/vendors/bootstrap-datepicker/bootstrap-datepicker.min.js"></script>
    <script src="~/template2/vendors/progressbar.js/progressbar.min.js"></script>

    <!-- End plugin js for this page -->
    <!-- inject:js -->
    <script src="~/template2/js/off-canvas.js"></script>
    <script src="~/template2/js/hoverable-collapse.js"></script>
    <script src="~/template2/js/template.js"></script>
    <script src="~/template2/js/settings.js"></script>
    <script src="~/template2/js/todolist.js"></script>
    <!-- endinject -->
    <!-- Custom js for this page-->
    <script src="~/template2/js/jquery.cookie.js" type="text/javascript"></script>
    <script src="~/template2/js/dashboard.js"></script>
    <script src="~/template2/js/Chart.roundedBarCharts.js"></script>
    <!-- End custom js for this page-->
@await RenderSectionAsync("menu", required: false)
    @await RenderSectionAsync("Scripts", required: false)
</body>

</html>

