{
  "version": "1.0",
  "defaultProvider": "cdnjs",
  "libraries": [
    {
      "library": "bootstrap@5.3.0",
      "destination": "wwwroot/lib/bootstrap/dist"
    },
    {
      "library": "jquery@3.6.1",
      "destination": "wwwroot/lib/jquery/dist"
    },
    {
      "library": "jquery-validate@1.19.5",
      "destination": "wwwroot/lib/jquery-validation/dist"
    },
    {
      "library": "jquery-validation-unobtrusive@4.0.0",
      "destination": "wwwroot/lib/jquery-validation-unobtrusive/dist"
    },
    {
      "library": "font-awesome@6.4.0",
      "destination": "wwwroot/lib/font-awesome/dist"
    },
    {
      "library": "bootstrap-icons@1.10.5",
      "destination": "wwwroot/lib/bootstrap-icons/dist"
    },
    {
      "provider": "unpkg",
      "library": "aos@2.3.4",
      "destination": "wwwroot/lib/aos"
    },
    {
      "provider": "jsdelivr",
      "library": "slick-carousel@1.8.1",
      "destination": "wwwroot/lib/slick-carousel"
    },
    {
      "provider": "jsdelivr",
      "library": "boxicons@2.1.4",
      "destination": "wwwroot/lib/boxicons"
    },
    {
      "library": "froala-editor@3.2.6",
      "destination": "wwwroot/lib/froala"
    },
    {
      "library": "animate.css@4.1.1",
      "destination": "wwwroot/lib/animate"
    },
    {
      "library": "select2@4.0.13",
      "destination": "wwwroot/lib/select2"
    },
    {
      "library": "tinymce@6.6.2",
      "destination": "wwwroot/lib/tiny",
      "files": [
        "tinymce.min.js",
        "plugins/**/*",
        "skins/**/*",
        "themes/**/*",
        "icons/**/*",
        "models/**/*",
        "langs/**/*"
      ]
    },
    {
      "provider": "jsdelivr",
      "library": "bootstrap-select@1.14.0-beta3",
      "destination": "wwwroot/lib/bootstrap-select"
    },
    {
      "library": "perfect-scrollbar@1.5.5",
      "destination": "wwwroot/lib/perfect-scrollbar"
    }
  ]
}